/* Overlay */
.gsv-hof-overlay {
	position: absolute;
	z-index: 10;
	left: -9999px;
	top: -9999px;
	width: 1px;
	height: 1px;
	background: url(data:image/png;base64,);
	opacity: 0;
}

.gsv-hof-overlay-close-button {
	cursor: pointer;
	display: block;
	position: absolute;
	top: 5px;
	left: 5px;
	width: 10px;
	height: 10px;
	background: url(data:image/png;base64,) no-repeat center center;
	padding: 10px;
	text-indent: -9999px;
	z-index: 11;
}	

.gsv-hof-overlay-info,
.gsv-hof-overlay-image,
.gsv-hof-overlay-heading,
.gsv-hof-overlay-heading-description,
.gsv-hof-overlay-summary,
.gsv-hof-overlay-content {
	display: none;
}

.gsv-hof-overlay-image,
.gsv-hof-overlay-quote {
	position: absolute;
	top: 0;
	right: 0;
	width: 480px;
}

.gsv-hof-overlay-quote {
	opacity: 0;
	width: 420px;
	padding: 0 30px;
	height: 180px;
	text-align: center;
	top: 270px;
}

.gsv-hof-overlay-quote p {
	color: #fff;
	font: 100 24px 'LithoAntiqueDemiBold';
	padding-bottom: 0;
	position: absolute;
	width: 420px;
	left: 50%;
	top: 50%;
	margin-left: -210px;
}

.gsv-hof-overlay-quote p:before {
	content: "\201C";
}

.gsv-hof-overlay-quote p:after {
	content: "\201D";
}

.gsv-hof-overlay-info {
	width: 400px;
	padding: 55px 40px 0;
}

.gsv-hof-overlay-heading,
.gsv-hof-overlay-heading-description {
	font: 40px/40px 'LithoAntiqueDemiBold';
	padding-bottom: 3px;
	text-transform: uppercase;
	color: #fff;
}

.gsv-hof-overlay-heading-description {
	font-size: 24px;
	text-transform: none;
	padding-bottom: 0;	
	padding-top: 2px;
	line-height: 20px;
}

.gsv-hof-overlay-summary,
.gsv-hof-notable-innovations-heading,
.gsv-hof-notable-innovations-list {
	position: absolute;
	left: 40px;
	width: 400px;
}

.gsv-hof-overlay-summary {
	color: #fff;
	top: 176px;
	font-size: 11px;
	line-height: 15px;
}

.gsv-hof-overlay-content {
	color: #fff;
}

.gsv-hof-notable-innovations-heading {
	color: #575757;
	text-transform: uppercase;
	padding-bottom: 0;
	font: 100 12px 'Arial', sans-serif;
	top: 268px;
}

.gsv-hof-notable-innovations-list {
	list-style: none;
	top: 300px;
}

.gsv-hof-notable-innovations-list-item {
	background: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAYAAAAGCAYAAAGXy9/eAAAAGXRFWHRTb2Z0d2FyZQBBZG9iZSBJbWFnZVJlYWR5ccllPAAAAGZJREFUeNpiCA8PZwAIIAYgkQAQQIxAQoCBgaEeIIBAvPdMQJYAQACBhPYDGQ5AzAAQQCBOP5AOAGJFFiBRwAAB9SC1B6CcRoAAAykDyfQzoAJDkCp+BkwgAJJoBFkAUgUyHMo+AACfdhA14+zCuQAAAABJRU5ErkJggg==) no-repeat left 5px;
	padding-left: 15px;
	font: 700 12px/12px 'Arial', sans-serif;
	padding-bottom: 10px;
	line-height: 15px;
}

.gsv-hof-categories {
	position: absolute;
	left: 0;
	bottom: 0;
	height: 35px;
	width: 960px;
	background: #000;
	font-size: 0;
}

.gsv-hof-categories-heading,
.gsv-hof-categories-list {
	float: left;	
	height: 25px;
	vertical-align: top;
}

.gsv-hof-categories-heading,
.gsv-hof-categories-list-item {
	border-right: 1px solid #454545;
}

.gsv-hof-categories-heading {
	width: 159px;
	text-align: center;
	color: #fff;
	text-transform: uppercase;
	font: 700 11px 'Arial', sans-serif;
	padding-top: 10px;
}

.gsv-hof-categories-list {
	list-style: none;
}

.gsv-hof-categories-list-item {
	width: 119px;
	float: left;
	cursor: default;
	text-align: center;
	height: 35px;
	font: 100 10px/35px 'Arial', sans-serif;
	color: #B0B0B0;
	-moz-transition: background-color .5s ease-out;
	-webkit-transition: background-color .5s ease-out;
	-ms-transition: background-color .5s ease-out;
	transition: background-color .5s ease-out;
}

.gsv-hof-categories-list-item:hover {
	-moz-transition: background-color .2s ease-in;
	-webkit-transition: background-color .2s ease-in;
	-ms-transition: background-color .2s ease-in;
	transition: background-color .2s ease-in;
}
