/*
Theme Name: GSV Capital
Author: Brand Lift 
Author URI: http://brand-lift.com
Version: 2.0
*/
body {
	font: 100 13px/19px 'Arial';
	color: #4b4b4b;
	min-width: 960px;
}

a {
	color: #26ae4c;
	text-decoration: none;
}

a:hover {
	color: #57c777;
}

ul,
ol,
p {
	padding-bottom: 22px;
}

em {
	font-style: italic;
}

strong {
	font-weight: 700;
}

input[type="text"],
input[type="email"],
textarea {
	padding: 10px;
	border: 1px solid #a6a28b;
	color: #ababab;
	width: 250px;
	font: 12px 'Arial';
}

input[type="text"],
input[type="email"] {
	height: 12px;
}

textarea {
	height: 190px;
	resize: none;
}

button[type="submit"] {
	cursor: pointer;
	display: inline-block;
	color: #fff;
	text-transform: uppercase;
	font-size: 11px;
	padding: 0 16px;
	height: 31px;
	line-height: 31px;
	text-transform: 1px 1px 1px rgba(0, 0, 0, .4);
	background: #28ce56; /* Old browsers */
	background-image: -moz-linear-gradient(top, #28ce56 0%, #1bb145 100%); /* FF3.6+ */
	background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#28ce56), color-stop(100%,#1bb145)); /* Chrome,Safari4+ */
	background-image: -webkit-linear-gradient(top, #28ce56 0%,#1bb145 100%); /* Chrome10+,Safari5.1+ */
	background-image: -o-linear-gradient(top, #28ce56 0%,#1bb145 100%); /* Opera11.10+ */
	background-image: -ms-linear-gradient(top, #28ce56 0%,#1bb145 100%); /* IE10+ */
	background-image: linear-gradient(top, #28ce56 0%,#1bb145 100%); /* W3C */
}

.gsv-required {
    color: #C31620;
    display: inline-block;
    font-size: 12px;
    font-style: italic;
    padding: 5px 0 0 5px;
    vertical-align: top;
  } 

/* Font Declarations */
@font-face {
    font-family: 'LithoAntiqueDemiBold';
    src: url('fonts/LithoAntique-DemiBold-webfont.eot');
    src: url('fonts/LithoAntique-DemiBold-webfont.eot?iefix') format('eot'),
         url('fonts/LithoAntique-DemiBold-webfont.woff') format('woff'),
         url('fonts/LithoAntique-DemiBold-webfont.ttf') format('truetype'),
         url('fonts/LithoAntique-DemiBold-webfont.svg#webfontR0N4lrQZ') format('svg');
    font-weight: normal;
    font-style: normal;
}

@font-face {
    font-family: 'LithoAntiqueMedium';
    src: url('fonts/LithoAntique-Medium-webfont.eot');
    src: url('fonts/LithoAntique-Medium-webfont.eot?iefix') format('eot'),
         url('fonts/LithoAntique-Medium-webfont.woff') format('woff'),
         url('fonts/LithoAntique-Medium-webfont.ttf') format('truetype'),
         url('fonts/LithoAntique-Medium-webfont.svg#webfontR0N4lrQZ') format('svg');
    font-weight: normal;
    font-style: normal;
}

/* Visual Hides */
.gsv-contact-us-flyout-form label,
.gsv-newsletter-signup-flyout-form label,
.gsv-about-leadership-heading {
	position: absolute;
	left: -9999px;
}

/* Misc global styles */
.gsv-home-carousel-list-item-learn-more-link,
.gsv-investment-portfolio-landing-client-list-item-more-link,
.gsv-tabset-additional-links-list-item-learn-more-link,
.gsv-about-leadership-list-item-learn-more-link,
.gsv-newsletter-signup-flyout-form-current-edition,
.gsv-moving-ideas-blog-list-item-story-read-more-link,
.gsv-home-carousel-list-item-blog-view-blog-link,
.gsv-moving-ideas-hof-footer-link {
	font: 700 11px/13px 'Arial';
	background: url(data:image/png;base64,) no-repeat right center;
	padding-right: 20px;
	-moz-transition: color .5s ease-out;
	-webkit-transition: color .5s ease-out;
	-ms-transition: color .5s ease-out;
	transition: color .5s ease-out;
	color: #969696;
}

.gsv-home-carousel-list-item-learn-more-link:hover,
.gsv-investment-portfolio-landing-client-list-item-more-link:hover,
.gsv-tabset-additional-links-list-item-learn-more-link:hover,
.gsv-home-carousel-list-item-blog-view-blog-link:hover {
	-moz-transition: color .2s ease-out;
	-webkit-transition: color .2s ease-out;
	-ms-transition: color .2s ease-out;
	transition: color .2s ease-out;
	color: #000;
}

.gsv-callout-statement {
	position: relative;
	right: -19px;
	height: 108px;
	float: right;
	padding: 50px 60px 0;
	margin-bottom: 75px;
	background: #e4ebff;
	font: 30px/31px 'LithoAntiqueDemiBold';
	color: #627ed1;
}

.gsv-callout-statement:before {
	content: "";
	background: url(data:image/png;base64,) no-repeat left top;
	position: absolute;
	left: -89px;
	top: 0;
	width: 89px;
	height: 158px;
	display: block;
}

.gsv-callout-statement p {
	padding-bottom: 0;
}

.gsv-main-content {
	float: right;
	width: 610px;
	padding-bottom: 60px;
}

.gsv-main-content-heading,
.gsv-main-content-subheading {
	color: #222;
	padding-bottom: 35px;
}


.gsv-main-content-heading {
	font: 35px 'LithoAntiqueDemiBold';
	text-transform: uppercase;	
}

.gsv-main-content-subheading {
	font: 700 18px/23px 'Arial';
}

.gsv-main-content-supporting-image {
	display: block;
	float: left;
	clear: right;
	margin: 10px 10px 0 0;
	width: 290px;
	text-align: center;
}

.gsv-main-content-col {
	display: block;
	float: left;
	width: 290px;
	padding-right: 30px;
}

.ie8 .gsv-main-content-col {
	width: 270px;
}

.gsv-main-content-col:last-child,
.gsv-main-content-col-last-child {
	padding-right: 0;
}

/* Tabset */
.gsv-tabset {
	position: relative;
}

.gsv-tabset-navigation-list {
	list-style-type: none;
	font-size: 0;
	border-bottom: 6px solid #222;
	padding-bottom: 0;
}

.gsv-tabset-navigation-list-item {
	display: inline-block;
	vertical-align: top;
	border-right: 1px solid #828282;
}

.gsv-tabset-navigation-list-item:last-child {
	border-right: none;
}

.gsv-tabset-navigation-list-item-link {
	display: block;
	width: 173px;
	height: 20px;
	color: #fff;
	padding: 23px 0 0 10px;
	font: 700 12px 'Arial';
	-moz-transition: background-color .2s ease-out;
	-webkit-transition: background-color .2s ease-out;
	-ms-transition: background-color .2s ease-out;
	transition: background-color .2s ease-out;
	background-color: #575757;
	position: relative;
}

.gsv-tabset-navigation-list-item:last-child .gsv-tabset-navigation-list-item-link {
	width: 174px;
}

.gsv-tabset-navigation-list-item-overview-link {
	padding-left: 50px;
	width: 133px;
	background: #575757 url(data:image/png;base64,) no-repeat 13px 9px;
}

.gsv-tabset-navigation-list-item-link:hover {
	-moz-transition: background-color .2s ease-out;
	-webkit-transition: background-color .2s ease-out;
	-ms-transition: background-color .2s ease-out;
	transition: background-color .2s ease-out;
	background-color: #6c6c6c;
	color: #fff;
}

.gsv-tabset-navigation-list-item-link.active,
.gsv-tabset-navigation-list-item-link.active:hover {
	background-color: #222;
	cursor: default;
}

.gsv-tabset-navigation-list-item-link.active:after {
	content: "";
	position: absolute;
	right: 10px;
	top: 27px;
	width: 5px;
	height: 7px;
	background: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAUAAAAHCAYAAADAp4fuAAAAGXRFWHRTb2Z0d2FyZQBBZG9iZSBJbWFnZVJlYWR5ccllPAAAAEpJREFUeNpi+P//vwsQMyBjELEbio2RBUOB+O5/COgAYkFkbWlA/A6Iy5EF7kIFQ0ECq6BaZ4K0ghSxMDAw3ANiVyDewwAFAAEGAOoZZuVsNU5AAAAAAElFTkSuQmCC) no-repeat left top;
}

.gsv-tabset-wrapper {
	height: 510px;
}

.gsv-tabset-content {
	display: none;
	position: absolute;
	left: 0;
	height: 410px;
	width: 840px;
	top: 49px;
	background-color: #dedede;
	padding: 50px 40px;
}

.gsv-tabset-content.active {
	display: block;
}

.gsv-tabset-content-heading {
	font: 26px 'LithoAntiqueDemiBold';
	padding-bottom: 25px;
	text-transform: uppercase;
	color: #222;
}

.gsv-tabset-content-col {
	width: 400px;
	float: left;
}

.gsv-tabset-content-col:last-child {
	padding-left: 40px;
}

.gsv-tabset-content-col-heading {
	font: 100 18px/22px 'Arial';
	padding-bottom: 10px;
	color: #4b4b4b;
}

.gsv-tabset-content-highlight-box {
	background: #e8e8e8;
	padding: 35px;
	color: #222;
	font: 12px/19px 'Arial';
}

.gsv-tabset-content-highlight-box p:last-child {
	padding-bottom: 0;
}

.gsv-tabset-content-highlight-box-heading {
	text-transform: uppercase;
	padding-bottom: 10px;
	color: #6682d6;
	font: 700 11px 'Arial';
}

/* Overview Tabset */
#gsv-tabset-overview .gsv-tabset-content-highlight-box .gsv-tabset-content-highlight-box-heading {
	font-size: 12px;
	color: #222;
	padding-bottom: 15px;
}

#gsv-tabset-overview .gsv-tabset-content-highlight-box p {
	font: 700 16px/19px 'Arial';
	color: #6682d6;	
}


/* People Tab */
.gsv-tabset-content-people-board-members-list-item,
.gsv-tabset-content-people-management-team-list-title {
	color: #222;
	font: 700 12px 'Arial';	
}

.gsv-tabset-content-people-board-members-list {
	list-style: none;
	line-height: 1;
}

.gsv-tabset-content-people-board-members-list-item {
	display: inline-block;
	vertical-align: baseline;
}

.gsv-tabset-content-people-board-members-list-item:after {
	content: ",";
	padding-right: 3px;
}

.gsv-tabset-content-people-board-members-list-item:last-child:after {
	content: "";
}

.gsv-tabset-content-people-management-team-list-definition {
	display: block;
	color: #4b4b4b;
	font: italic 12px 'Arial';
	padding-bottom: 15px;
}

/* Potential Tab */
.gsv-tabset-content-highlight-box-col {
	float: left;
	width: 150px;
}

.gsv-tabset-content-highlight-box-col:last-child {
	float: right;
}

.gsv-tabset-content-potential-megatrends-list {
	list-style-type: none;
	line-height: 1;
}

.gsv-tabset-content-potential-megatrends-list-item,
.gsv-main-content-advisory-board-list-list-item {
	position: relative;
	padding: 0 0 15px 15px;
}

.gsv-tabset-content-potential-megatrends-list-item:before,
.gsv-main-content-advisory-board-list-list-item:before {
	content: "";
	position: absolute;
	left: 0;
	top: 3px;
	display: block;
	border: 3px solid #6682d6;
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	-o-border-radius: 3px;
	border-radius: 3px;
}

.gsv-tabset-additional-links-list {
	list-style-type: none;
	border-color: transparent #dedede #dedede;
	border-width: 0 1px 1px;
	border-style: solid;
	padding-bottom: 0;
}

.gsv-tabset-additional-links-list-item-report-link,
.gsv-tabset-additional-links-list-item-learn-more-link {
	display: block;
	height: 45px;
	font: 700 14px/45px 'Arial';
}

.gsv-tabset-additional-links-list-item-report-link {
	float: left;
	margin-left: 40px;
	border-left: 1px solid #e0e0e0;
	border-right: 1px solid #e0e0e0;
	padding: 0 20px 0 45px;
	color: #222;
	background: url(data:image/png;base64,) no-repeat 20px center;
}

.gsv-tabset-additional-links-list-item-learn-more-link {
	float: right;
	margin-right: 20px;
}

/* Overlays */
.gsv-modal {
    background: #000;
    opacity: .75;
    filter:progid:DXImageTransform.Microsoft.Alpha(opacity=75);
    display: none;
    height: 100%;
    left: 0;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 5;
}

.gsv-flyout {
    background-color: #fff;
    border: 10px solid #E0E0E0;
    display: none;
    left: 50%;
    padding: 55px 40px;
    position: fixed;
    top: 50%;
    z-index: 6;
}

.gsv-flyout-close-link {
    background-color: #E0E0E0;
    color: #fff;
    cursor: pointer;
    font-weight: 700;
    padding: 5px 10px;
    position: absolute;
    right: -2px;
    text-transform: uppercase;
    top: -5px;
}

.gsv-flyout-heading {
    font: 30px/21px 'LithoAntiqueDemiBold';
    text-transform: uppercase;
    color: #000;
    margin-bottom: 25px;
}

/* Breadcrumbs */
.gsv-breadcrumbs-list {
	list-style: none;
	display: block;
	position: absolute;
	left: -11px;
	top: 60px;
	padding-bottom: 0;
	font: 700 12px/29px 'Arial';
	color: #fff;
	text-transform: uppercase;
}

.gsv-breadcrumbs-list:after {
	position: absolute;
	left: 0;
	top: 29px;
	content: "";
	display: block;
	height: 11px;
	width: 11px;
	background: url(data:image/png;base64,) no-repeat left top;
}

.gsv-breadcrumbs-list-item {
	display: block;
	float: left;
	background: #6682d6;
	padding: 0 10px;
	margin: 0 16px;
	height: 29px;
	position: relative;
}

.ie8 .gsv-breadcrumbs-list-item {
	margin-left: 0;
}

.gsv-breadcrumbs-list-item:first-child {
	margin-left: 0;
	padding-left: 35px;
}

.gsv-breadcrumbs-list-item:first-child:before {
	display: none;
}

.gsv-breadcrumbs-list-item:before,
.gsv-breadcrumbs-list-item:after {
	position: absolute;
	content: "";
	top: 0;
	display: block;
	height: 29px;
	width: 16px;
}

.gsv-breadcrumbs-list-item:before {
	left: -16px;
	background: url(data:image/png;base64,) no-repeat left top;
}

.gsv-breadcrumbs-list-item:after {
	right: -16px;
	background: url(data:image/png;base64,) no-repeat left top;
}

.gsv-breadcrumbs-list-item:nth-child(2) {
	left: -16px;
	opacity: .85;
}

.gsv-breadcrumbs-list-item:nth-child(3) {
	left: -32px;
	opacity: .7;
}

.gsv-breadcrumbs-list-item-link,
.gsv-breadcrumbs-list-item-heading {
	background: url(data:image/png;base64,) no-repeat left center;
	padding-left: 17px;
	opacity: 1;
	color: #fff;
}

.gsv-breadcrumbs-list-item:first-child .gsv-breadcrumbs-list-item-link,
.gsv-breadcrumbs-list-item:first-child .gsv-breadcrumbs-list-item-heading {
	background: none;
	padding-left: 0;
}

.gsv-breadcrumbs-list-item .gsv-breadcrumbs-list-item-link:hover {
	color: #becbf0;
}

/* Site Structure */
.gsv-container {
	overflow: hidden;
	width: 100%;
}

.gsv-header-contents,
.gsv-gravity-contents,
.gsv-news-and-social-contents,
.gsv-home-carousel,
.gsv-footer-contents,
.gsv-content-contents {
	width: 960px;
	margin: 0 auto;
	position: relative;
}

.gsv-content {
	background: #f4f4f4;
}

/* GL */
.ie7 .gsv-content {
	position: relative;
	z-index: 1;
}

.gsv-content-contents {
	width: 920px;
	background: #fff;
	padding: 125px 19px 0;
	border-right: 1px solid #dedede;
	border-left: 1px solid #dedede;
}

.gsv-content-contents-with-grounding {
	padding-top: 225px;	
}

.gsv-content-contents-with-grounding-double {
	padding-top: 281px;	
}

.gsv-content-subheading {
	font: 50px/39px 'LithoAntiqueDemiBold';
	padding-bottom: 45px;
	color: #222;
	text-transform: uppercase;
}

.gsv-content-subheading-grounding {
	padding-bottom: 30px;
	border-bottom: 1px solid #DEDEDE;
	position: absolute;
	top: 125px;
	left: 0;
	padding-left: 19px;
	width: 940px;
}

.gsv-content-subheading-grounding-double {
	line-height: 52px;
	padding-bottom: 22px;
}

/* Site Header */
.gsv-header {
	background: #fff;
	border-bottom: 6px solid #000;
	height: 100px;
}
/* GL */
.ie7 .gsv-header {
	position: relative;
	z-index: 2;
}

.gsv-header-logo-link {
	display: block;
	margin-top: 16px;
	float: left;
}

.gsv-header-logo-image {
	display: block;
}

.gsv-header-navigation-list {
	list-style: none;
	font-size: 0;
	float: right;
	padding-bottom: 0;
	border-right: 1px solid #d4d4d4;
}

.gsv-header-navigation-list-item {
	display: inline-block;
	vertical-align: top;
	font-size: 12px;
	-moz-transition: background-color .5s ease-out;
	-webkit-transition: background-color .5s ease-out;
	-ms-transition: background-color .5s ease-out;
	transition: background-color .5s ease-out;
	background-color: #fff;
	color: #323334;
	cursor: pointer;
}
/* GL */
.ie7 .gsv-header-navigation-list-item {
	zoom: 1;
	*display: inline;
	_height:100px;
}
.gsv-header-navigation-list-item:hover {
	-moz-transition: background-color .2s ease-in;
	-webkit-transition: background-color .2s ease-in;
	-ms-transition: background-color .2s ease-in;
	transition: background-color .2s ease-in;
	background-color: #6682d6;
}

.gsv-header-navigation-list-item.active {
	background-color: #6682d6;
}

.gsv-header-navigation-list-item:hover .gsv-header-navigation-list-item-link {
	-moz-transition: color .2s ease-in;
	-webkit-transition: color .2s ease-in;
	-ms-transition: color .2s ease-in;
	transition: color .2s ease-in;
	color: #fff;
}

.gsv-header-navigation-list-item.active .gsv-header-navigation-list-item-link {
	color: #fff;
}

.gsv-header-navigation-list-item-link {
	display: block;
	border-left: 1px solid #d4d4d4;
	height: 40px;
	width: 145px;
    padding: 60px 0 0 15px;
   	font: 700 12px/13px 'Arial';
   	-moz-transition: color .5s ease-out;
	-webkit-transition: color .5s ease-out;
	-ms-transition: color .5s ease-out;
	transition: color .5s ease-out;
	color: #323334;
}

.gsv-header-navigation-list-item-home:hover {
	background-color: transparent;
}

.gsv-header-navigation-list-item-link-home,
.gsv-header-navigation-list-item-link-home:hover {
	-moz-transition: opacity .2s ease-in;
	-webkit-transition: opacity .2s ease-in;
	-ms-transition: opacity .2s ease-in;
	transition: opacity .2s ease-in;
	background: transparent url(data:image/png;base64,) no-repeat left top;
	cursor: pointer;
}

.gsv-header-navigation-list-item-link-home {
	opacity: .7;
	width: 16px;
	height: 14px;
	display: block;
	padding: 0;
	margin: 70px 20px 0 0;
	border-left: none;
	text-indent: -9999px;
}

.gsv-header-navigation-list-item-link-home:hover {
	opacity: 1;
}

/* Dropdowns */
.gsv-header-navigation-dropdown {
	display: none;
	position: absolute;
	left: 0;
	top: 100px;
	border-top: 6px solid #6682d6;
	padding: 15px;
	background: #d2d2d2;
	width: 930px;
	z-index: 2;
	font-size: 0;
}

.gsv-header-dropdown-navigation-list,
.gsv-header-dropdown-sub-navigation-list {
	list-style: none;
	padding-bottom: 0;
}

.gsv-header-navigation-dropdown-four-ps,
.gsv-header-navigation-dropdown-our-investments,
.gsv-header-dropdown-navigation-list-item {
	display: inline-block;
	vertical-align: top;
	font-size: 12px;
}

/* GL */
.ie7 .gsv-header-navigation-dropdown-four-ps,
.ie7 .gsv-header-navigation-dropdown-our-investments,
.ie7 .gsv-header-dropdown-navigation-list-item {
	zoom:1; 
	*display: inline;
	 _height: 30px;
	 float: left;
}


.gsv-header-dropdown-navigation-list-item {
	width: 185px;
	margin-right: 1px;
}

.gsv-header-navigation-dropdown-four-ps-heading,
.gsv-header-navigation-dropdown-our-investments-heading,
.gsv-header-dropdown-navigation-list-item-link {
	display: block;
	height: 35px;
	font: 700 13px/35px 'Arial';
	color: #000;
	padding-left: 15px;
	margin-bottom: 1px;
	background: #eae9e9;
}

.gsv-header-navigation-dropdown-our-investments-list-item .gsv-header-navigation-dropdown-our-investments-list-item-link-control4 { cursor: default; color: #999; }
.gsv-header-navigation-dropdown-our-investments-list-item .gsv-header-navigation-dropdown-our-investments-list-item-link-control4:hover { background: #fff; color: #999;}
.gsv-header-navigation-dropdown-our-investments-list-item-link,
.gsv-header-dropdown-sub-navigation-list-item-link {
	color: #474747;
	display: block;
	height: 35px;
	width: 170px;
	padding-left: 15px;
