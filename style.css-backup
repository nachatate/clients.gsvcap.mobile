.gsv-hof-list {
	left: 0;
	top: 0;
	position: absolute;
	width: 480px;
	height: 270px;
	padding: 0;
	list-style: none;
}

.gsv-hof-list-item {
	/*-webkit-perspective: 2500px;
	-moz-perspective: 2500px;
	*/
}

.ios-device .gsv-hof-list-item {
	-webkit-perspective: 0;
	-moz-perspective: 0;
}


.gsv-hof-list-item-wrapper {
	position: absolute;
	left: -9999px;
	cursor: pointer;
	z-index: 1;
	-moz-transform-style: preserve-3d;	
	-moz-backface-visibility: hidden;
	-webkit-transform-style: preserve-3d;	
	-webkit-backface-visibility: hidden;
	-moz-transition: opacity .5s ease-out;
	-webkit-transition: opacity .5s ease-out;
	transition: opacity .5s ease-out;
}

.gsv-hof-list-item-wrapper-inactive {
	-moz-transition: opacity .2s ease-in;
	-webkit-transition: opacity .2s ease-in;
	transition: opacity .2s ease-in;
	opacity: .15;
}


.gsv-hof-list-item-wrapper-invisible,
.gsv-hof-list-item-wrapper-visible {
	-moz-transition-property: -moz-transform;
	-moz-transition-duration: .5s;	
	-webkit-transition-property: -webkit-transform;
	-webkit-transition-duration: .5s;
}

.gsv-hof-flip .gsv-hof-list-item-wrapper-visible {
	-moz-transition-timing-function: ease-out;
	-moz-transition-delay: .5s;
	-moz-transform: rotateY(0);
	-webkit-transition-timing-function: ease-out;
	-webkit-transition-delay: .5s;
	-webkit-transform: rotateY(0);
}

.gsv-hof-flip .gsv-hof-list-item-wrapper-invisible {
	-moz-transform: rotateY(90deg);
	-moz-transition-timing-function: ease-in;
	-webkit-transform: rotateY(90deg);
	-webkit-transition-timing-function: ease-in;
	opacity: 1;
}

.gsv-hof-list-item-wrapper-invisible {
	-moz-transform: none;
	-moz-transform: rotateY(-90deg);
	-webkit-transform: none;
	-webkit-transform: rotateY(-90deg);
	opacity: 0;
	
}



.gsv-hof-list-item-info {
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	-moz-transition: opacity .5s ease-out;
	-webkit-transition: opacity .5s ease-out;
	transition: opacity .5s ease-out;
	opacity: 0;
	display: none;
}
/* GL */
.ie .gsv-hof-list-item-info {
	background: url(i/transparent.png) repeat;
}


.gsv-hof-list-item-wrapper:hover .gsv-hof-list-item-info {
	-moz-transition: opacity .1s ease-in;
	-webkit-transition: opacity .1s ease-in;
	transition: opacity .1s ease-in;
	opacity: 1;
	display: block;
}

